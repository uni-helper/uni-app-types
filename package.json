{
  "name": "@uni-helper/uni-app-types",
  "version": "0.3.2",
  "description": "uni-app 组件类型",
  "keywords": [
    "uni-app",
    "uniapp",
    "type"
  ],
  "homepage": "https://github.com/uni-helper/uni-app-types#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-app-types/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-app-types.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "types"
  ],
  "scripts": {
    "release": "release-it",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "lint": "run-p lint:eslint lint:markdownlint check:types test:coverage",
    "lint:eslint": "eslint ./types --fix --cache --no-error-on-unmatched-pattern",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "test": "vitest typecheck --run",
    "test:coverage": "vitest typecheck --run --coverage",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@dcloudio/types": "^3.2.10",
    "vue3": "npm:vue@^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt": "^17.4.2",
    "@modyqyw/fabric": "^6.0.1",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitest/coverage-c8": "^0.28.3",
    "c8": "^7.12.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "release-it": "^15.6.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.3"
  },
  "peerDependencies": {
    "typescript": "^4.8.0"
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
